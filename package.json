{
  "name": "vite-ts-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "vite build",
    "preview": "vite preview  --open",
    "lint:check": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix --report-unused-disable-directives --max-warnings 0",
    "prettier:check": "prettier . --check . ",
    "prettier:format": "prettier \"**/*.{html,css,js,jsx,json,md}\" --cache --write",
    "husky:prepare": "husky",
    "husky:pre-commit": "echo npx lint-staged > .husky/pre-commit",
    "husky:commit-msg": "echo npx --no-install commitlint --edit \"$1\" > .husky/commit-msg",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "antd": "^5.24.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-config-eslint": "^11.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
